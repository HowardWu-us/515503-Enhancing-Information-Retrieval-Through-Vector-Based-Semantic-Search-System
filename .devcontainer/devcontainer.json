{
    "name": "LaTeX Environment",
    "context": "..",
    "dockerFile": "Dockerfile",
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
        "mechatroner.ruby",
        "James-Yu.latex-workshop"
    ],
    "postCreateCommand": "tex --version",
    "forwardPorts": [
        5000
    ],
    "features": {
        "ghcr.io/devcontainers/features/python:1": {
            "installTools": true,
            "version": "3.9"
        }
    }
}
